template <class T>
void sortArray(T a[], int n)
{ 
	//Add your code here.
	for(int i=0;i<n;i++){
	    for(size_t j=i+1;j<n;j++){
	        if(a[i]>a[j]){
	            T temp = a[i];
	            a[i] = a[j];
	            a[j] = temp;
	        }
	    }
	}
}
template <class T>
void printArray(T a[], int n)
{
	//Add your code here.
	for(size_t i=0;i<n;i++)
	{cout<<a[i]<<" ";
	}
	cout<<endl;
}

//Tags : Bits

#include <iostream>
using namespace std;

int main() {
	//code
	int t;
	cin>>t;
	while(t--){
	    int n;
	    cin>>n;
	    int a[8]={0};
	    int i=0,count=0;
	    while(n!=0){
	        a[i] = n%2;
	        n = n/2;
	        i++;
	    }
	    for(int i=0;i<8;i++){
	        if(a[i]==1){
	            count++;
	        }
	    }
	    cout<<count<<endl;
	}
	return 0;
}
